# Supabase Configuration
# Get your Supabase URL and keys from: https://app.supabase.com/project/_/settings/api
EXPO_PUBLIC_SUPABASE_URL=your_supabase_url_here
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Multi-Tenant Configuration (if using multi-tenant mode)
EXPO_PUBLIC_MULTI_TENANT=false
EXPO_PUBLIC_TENANT_ID=your_tenant_id_here
EXPO_PUBLIC_TENANT_REGISTRY_URL=your_tenant_registry_url_here
EXPO_PUBLIC_TENANT_REGISTRY_ANON_KEY=your_tenant_registry_anon_key_here

# Google Gemini AI Configuration (for AI insights)
# Get your API key from: https://aistudio.google.com/app/apikey
EXPO_PUBLIC_GEMINI_API_KEY=your_gemini_api_key_here

# Optional: ElevenLabs (for voice features)
# Get your agent ID from: https://elevenlabs.io/app/conversational-ai/agents
EXPO_PUBLIC_AGENT_ID=your_agent_id_here
EXPO_PUBLIC_ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# App Configuration
EXPO_PUBLIC_APP_NAME=Pulse of People
EXPO_PUBLIC_APP_VERSION=1.0.0
EXPO_PUBLIC_DEFAULT_LANGUAGE=en
